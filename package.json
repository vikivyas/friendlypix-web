    "concurrently": "^5.1.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "firebase-tools": "^8.1.1",
    "glob-all": "^3.2.1",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "purify-css": "^1.2.5",
